alias NAME r15
alias gasfilter r14
alias gasratio r13
alias solltemp r12
alias setting r11
define PUMPE 1310794736
define VALVE -1280984102
define TANK 272136332
l NAME db NameHash
s db Setting 0
main:
#druckablassen
lbn r2 TANK NAME TotalMoles 0
sgt r1 r2 15000
s d0 On r1
#ausgangspumpe
l r2 db PressureOutput
sgt r1 r2 1
sbn PUMPE NAME On r1
#finde raus welche Filter am slot 0
ls gasfilter db 0 PrefabHash
beq gasfilter 1635000764 isCO2
beq gasfilter -147575240 isCO2
beq gasfilter 1876847024 isCO2
beq gasfilter 632853248 isN2
beq gasfilter 1033488730 isN2
beq gasfilter -1387439451 isN2
beq gasfilter -1247674305 isN2O
beq gasfilter -2072216503 isN2O
beq gasfilter 465267979 isN2O
beq gasfilter -721824748 isO2
beq gasfilter -101452292 isO2
beq gasfilter -1217998945 isO2
beq gasfilter 1915566057 isX
beq gasfilter -1636159526 isX
beq gasfilter 1959564765 isX
beq gasfilter 15011598 isH2
beq gasfilter 1899166295 isH2
beq gasfilter 1255156286 isH2
beq gasfilter -1993197973 isH2O
beq gasfilter -1089201089 isH2O
beq gasfilter 2004969680 isH2O
j isnofilter
afterfilter:
#kuehlung
lbn r2 TANK NAME Temperature 0
sgt r1 r2 solltemp
sbn VALVE NAME On r1
#wenn unter 10Mpa nur ein filter an
l r2 db PressureInput
brlt r2 10000 unter
brge r2 10000 ueber
j main
unter:
j main
isnofilter:
s db Setting 99
s db Mode 0
j main
ueber:
s db Setting setting
sgt r1 gasratio 0
s db Mode r1
j main
isCO2:
move setting 1
l gasratio db RatioCarbonDioxideInput
move solltemp 323
j afterfilter
isN2:
move setting 2
l gasratio db RatioNitrogenInput
move solltemp 293
j afterfilter
isN2O:
move setting 3
l gasratio db RatioNitrousOxideInput
move solltemp 473
j afterfilter
isO2:
move setting 4
l gasratio db RatioOxygenInput
move solltemp 293
j afterfilter
isX:
move setting 5
l gasratio db RatioPollutantInput
move solltemp 473
j afterfilter
isH2:
move setting 6
l gasratio db RatioVolatilesInput
move solltemp 293
j afterfilter
isH2O:
move setting 7
l gasratio db RatioWaterInput
move solltemp 473
j afterfilter
